{
  "name": "rhubarbvr-luncher",
  "version": "1.0.0",
  "description": "The Luncher and updater for RhubarbVR",
  "productName": "RhubarbVR",
  "author": "Faolan",
  "main": "main.js",
  "scripts": {
    "start": "electron ./",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "protocols": {
      "name": "rhubarbvr-protocol",
      "schemes": [
        "rhubarbvr"
      ]
    },
    "appId": "RhubarbVR",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "runAfterFinish": true,
      "createDesktopShortcut": true
    },
    "win": {
      "icon": "C:\\Users\\TheRa\\Desktop\\New folder (9)\\RhubarbVRICON.png",
      "target": "NSIS"
    }
  },
  "repository": "",
  "devDependencies": {
    "electron": "^11.2.3",
    "electron-builder": "^20.38.0"
  },
  "dependencies": {
    "ajv": "^6.12.6",
    "custom-electron-titlebar": "^3.2.6",
    "electron-builder-squirrel-windows": "^22.10.4",
    "express": "^4.17.1",
    "fs": "0.0.1-security",
    "https": "^1.0.0",
    "nedb": "^1.8.0",
    "node": "^8.17.0",
    "node-fetch": "^2.6.1",
    "xmlhttprequest": "^1.8.0"
  }
}
